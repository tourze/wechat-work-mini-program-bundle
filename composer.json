{
  "name": "tourze/wechat-work-mini-program-bundle",
  "description": "A Symfony Bundle for WeChat Work Mini Program integration",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/event-dispatcher-contracts": "^3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/symfony-dependency-service-loader": "1.*",
    "tourze/wechat-work-bundle": "0.1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "WechatWorkMiniProgramBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WechatWorkMiniProgramBundle\\Tests\\": "tests"
    }
  }
}